  <body class="bannerA">
  <div id = "castle" class="bannerC">
    <nav class="navbar navbar-expand-lg navbar-dark" id="mainbar">
      <div>
        <div class="container-fluid">
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a
                  class="nav-link active"
                  aria-current="page"
                  href="/"
                  id="home"
                >Home</a>
              </li>
              <li class="nav-item">
                {{#if loggedIn}}
                  <a
                    class="nav-link active"
                    aria-current="page"
                    href="/dashboard"
                    id="casino"
                  >Casino</a>
                {{else}}
                  <a
                    class="nav-link active"
                    aria-current="page"
                    href="/login"
                    id="activelogin"
                  >Casino</a>
                {{/if}}
              </li>
              {{#if loggedIn}}
                <li class="nav-item">
                  <a
                    id="logout"
                    class="nav-link active"
                    aria-current="page"
                    href="#"
                    id="logout"
                  >Logout</a>
                </li>
              {{else}}
                <li class="nav-item">
                  <a
                    class="nav-link active"
                    aria-current="page"
                    href="/login"
                    id="inactivelogin"
                  >Login</a>
                </li>
              {{/if}}
            </ul>
          </div>
        </div>
      </div>
    </nav>
    <br />
    <div class="m-3 text-center">
      {{{pageTitle}}}
    </div>

